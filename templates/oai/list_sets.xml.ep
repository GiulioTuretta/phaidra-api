%= include 'oai/header'
<ListSets>
<% for my $setSpec (keys %{$sets}) { %>
  <set>
    <setSpec><%= $sets->{$setSpec}->{setSpec} %></setSpec>
    <setName><%= $sets->{$setSpec}->{setName} %></setName>
    <setDescription>
%= include 'oai/oai_dc', r => $sets->{$setSpec}->{setDescription}, fields => $sets->{$setSpec}->{fields}
    </setDescription>
  </set>
<% } %>
</ListSets>
%= include 'oai/footer'
